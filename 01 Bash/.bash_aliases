# common commands
alias ..="cd .."
alias ....="cd ../.."
alias ....="cd ../.."
alias ......="cd ../../.."
alias ll="ls -laho"
alias lm="ls | more"
alias la="ls -alFh --group-directories-firs"
alias cls="clear"
# space disk
alias df="df -h"
alias du="du -h"
alias free="free -mt"
alias psa="ps auxf"
alias cputemp="sensors | grep Core"
# MJalias
alias mja="cat ~/.bash_aliases"
alias emja="vim ~/.bash_aliases"
alias smja="source ~/.bash_aliases"
