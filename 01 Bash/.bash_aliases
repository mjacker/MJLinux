#!/usr/bin/env bash
# common commands
alias ..="cd .."
alias ....="cd ../.."
alias ....="cd ../.."
alias ......="cd ../../.."
alias ll="ls -ha"
alias lm="ls | more"
alias la="ls -alFh --group-directories-firs"

alias cls="clear"

# space disk
alias df="df -h"
alias du="du -h"
alias free="free -mt"
alias psa="ps auxf"
alias cputemp="sensors | grep Core"

# MJalias
alias mja="cat ~/.bashrc"
alias emja="vim ~/.bashrc"
alias smja="source ~/.bashrc"

# Paths
alias caps="cd /data/data/com.termux/files/home/mjtermux/01_mjcapstone/MJCapstone"

# Custom apps
alias w3="w3m www.google.com"
